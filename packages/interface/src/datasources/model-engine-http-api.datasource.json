{
  "name": "ModelEngineHttpApi",
  "connector": "rest",
  "baseURL": "http://model-engine",
  "options": {
    "headers": {
      "accept": "application/json",
      "content-type": "application/json"
    }
  },
  "crud": false,
  "operations": [
    {
      "template": {
        "method": "POST",
        "url": "http://model-engine/character/process",
        "body": "{req}"
      },
      "functions": {
        "processCharacter": ["req"]
      }
    },
    {
      "template": {
        "method": "POST",
        "url": "http://model-engine/location/process",
        "body": "{req}"
      },
      "functions": {
        "processLocation": ["req"]
      }
    },
    {
      "template": {
        "method": "POST",
        "url": "http://model-engine/qr/process",
        "body": "{req}"
      },
      "functions": {
        "processQr": ["req"]
      }
    },
    {
      "template": {
        "method": "POST",
        "url": "http://model-engine/character/default",
        "body": "{req}"
      },
      "functions": {
        "defaultCharacter": ["req"]
      }
    }
  ]
}
