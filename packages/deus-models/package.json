{
  "name": "deus-models",
  "version": "0.0.1",
  "description": "Deus Ex Model Server — handlers that process events.",
  "main": "index.js",
  "scripts": {
    "build": "tsc -b",
    "test": "mocha",
    "coverage:test": "nyc --cwd=.. --temp-dir=deus-models/.nyc_output mocha",
    "coverage:merge": "nyc merge .nyc_output ../../.nyc_output/deus-models.coverage.json",
    "lint": "yarn run prettier:check && yarn run tslint",
    "lint:fix": "yarn run tslint:fix && yarn run prettier:fix",
    "prettier:cli": "prettier --ignore-path=../../.prettierignore  \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "tslint": "lb-tslint -c ../../tslint.json",
    "tslint:fix": "yarn run tslint --fix",
    "prestart": "yarn run build",
    "start": "node -r tsconfig-paths/register .",
    "docker:build": "docker build -t sr2020/deus-models -f Dockerfile ../..",
    "docker:push": "docker push sr2020/deus-models"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sth-larp/deus-models.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sth-larp/deus-models/issues"
  },
  "homepage": "https://github.com/sth-larp/deus-models#readme",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/node": "^8.0.4",
    "chai": "^4.0.2",
    "chokidar-cli": "^1.2.0"
  },
  "dependencies": {
    "@types/chance": "^1.0.0",
    "@types/clone": "^0.1.30",
    "@types/lodash": "^4.14.106",
    "async-file": "^2.0.2",
    "chance": "^1.0.10",
    "clone": "^1.0.4",
    "fs.realpath": "^1.0.0",
    "inflight": "^1.0.6",
    "lodash": "^4.17.4",
    "source-map-support": "^0.4.15",
    "type-detect": "^4.0.3",
    "yn": "^2.0.0"
  }
}
