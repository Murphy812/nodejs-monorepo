{
  "name": "sr2020-testing",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build": "tsc -b",
    "test": "jest --config jest.config.ts --forceExit",
    "test:realdb": "cross-env NODE_ENV=prod jest --forceExit",
    "coverage:test": "nyc --cwd=.. --temp-dir=sr2020-testing/.nyc_output mocha --exit",
    "coverage:merge": "nyc merge .nyc_output ../../.nyc_output/sr2020-testing.coverage.json",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "prettier --ignore-path=../../.prettierignore  \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint -c ../../.eslintrc.js --ignore-path=../../.eslintignore --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {},
  "dependencies": {
    "cross-env": "^5.2.0"
  }
}
