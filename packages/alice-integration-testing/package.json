{
  "name": "alice-integration-testing",
  "version": "1.0.0",
  "description": "integration testing tools",
  "scripts": {
    "deps": "yarn install",
    "build": "tsc",
    "lint": "yarn run prettier:check && yarn run tslint",
    "lint:fix": "yarn run tslint:fix && yarn run prettier:fix",
    "prettier:cli": "prettier --ignore-path=../../.prettierignore  \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "tslint": "lb-tslint -c ../../tslint.json",
    "tslint:fix": "yarn run tslint --fix"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/command-line-args": "^4.0.0",
    "@types/request": "0.0.45",
    "@types/request-promise": "^4.1.35",
    "command-line-args": "^4.0.6",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  }
}
