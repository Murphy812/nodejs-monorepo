{
  "name": "telegram-bot",
  "version": "1.0.0",
  "description": "Telegram bot",
  "main": "index.js",
  "scripts": {
    "deps": "yarn install",
    "build": "tsc",
    "start": "yarn run build && forever start ./main.js",
    "stop": "forever stop ./main.js",
    "prettier:cli": "prettier --ignore-path=../../.prettierignore  \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint -c ../../.eslintrc.js --ignore-path=../../.eslintignore --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/node-telegram-bot-api": "^0.30.3",
    "@types/qrcode": "^1.3.0",
    "@types/uuid": "^3.4.4",
    "dotenv": "^6.1.0",
    "node-telegram-bot-api": "^0.30.0",
    "qrcode": "^1.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {}
}
