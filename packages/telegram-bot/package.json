{
  "name": "telegram-bot",
  "version": "1.0.0",
  "description": "Telegram bot",
  "main": "index.js",
  "scripts": {
    "deps": "yarn install",
    "build": "tsc",
    "start": "yarn run build && forever start ./main.js",
    "stop": "forever stop ./main.js",
    "lint": "yarn run prettier:check && yarn run tslint",
    "lint:fix": "yarn run tslint:fix && yarn run prettier:fix",
    "prettier:cli": "prettier \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "tslint": "lb-tslint -c ../../tslint.json",
    "tslint:fix": "yarn run tslint --fix"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/node-telegram-bot-api": "^0.30.3",
    "@types/qrcode": "^1.3.0",
    "@types/uuid": "^3.4.4",
    "dotenv": "^6.1.0",
    "node-telegram-bot-api": "^0.30.0",
    "qrcode": "^1.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {}
}
