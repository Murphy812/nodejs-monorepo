{
  "name": "billing",
  "version": "1.0.1",
  "description": "billing",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "tsc -b",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist && rm -f *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "yarn run build",
    "test": "lb-mocha --allow-console-logs \"src/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs src/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t billing .",
    "docker:run": "docker run -p 3000:3000 -d billing",
    "migrate": "node ./src/migrate",
    "prestart": "npm run build",
    "start": "node ./src",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {},
  "devDependencies": {}
}
