{
  "name": "billing",
  "version": "1.0.2",
  "description": "billing",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "tsc -b",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist && rm -f *.tsbuildinfo",
    "lint": "yarn run prettier:check && yarn run tslint",
    "lint:fix": "yarn run tslint:fix && yarn run prettier:fix",
    "prettier:cli": "prettier \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -- -l",
    "prettier:fix": "yarn run prettier:cli -- --write",
    "tslint": "lb-tslint -c ../../tslint.json",
    "tslint:fix": "yarn run tslint -- --fix",
    "pretest": "yarn run build",
    "test": "lb-mocha --allow-console-logs \"src/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs src/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t sr2020/billing -f Dockerfile ../..",
    "docker:push": "docker push sr2020/billing",
    "migrate": "node -r tsconfig-paths/register ./src/migrate",
    "start": "node -r tsconfig-paths/register ./src"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {}
}
