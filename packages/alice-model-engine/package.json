{
  "name": "alice-model-engine",
  "version": "0.0.1",
  "description": "ALICE model scripts runner",
  "main": "dist/worker.js",
  "scripts": {
    "deps": "yarn install",
    "build": "tsc -b",
    "test": "mocha",
    "coverage:test": "nyc --cwd=.. --temp-dir=alice-model-engine/.nyc_output mocha",
    "coverage:merge": "nyc merge .nyc_output ../../.nyc_output/alice-model-engine.coverage.json",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "prettier --ignore-path=../../.prettierignore  \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint -c ../../.eslintrc.js --ignore-path=../../.eslintignore --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "chai": "^4.1.2"
  },
  "dependencies": {
    "@types/cuid": "^1.3.0",
    "@types/glob": "^5.0.35",
    "@types/lodash": "^4.14.106",
    "@types/meow": "^4.0.1",
    "@types/node": "^9.6.1",
    "@types/systemjs": "^0.20.6",
    "cuid": "^2.1.0",
    "glob": "^7.1.1",
    "lodash": "^4.17.5",
    "meow": "^4.0.0"
  }
}
