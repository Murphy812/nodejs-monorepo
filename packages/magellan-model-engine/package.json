{
  "name": "magellan-model-engine",
  "version": "0.0.1",
  "description": "Models for Magellan 2018 LARP.",
  "main": "index.js",
  "scripts": {
    "deps": "yarn install",
    "build": "tsc -b",
    "test": "mocha",
    "coverage:test": "nyc --cwd=.. --temp-dir=magellan-model-engine/.nyc_output mocha",
    "coverage:merge": "nyc merge .nyc_output ../../.nyc_output/magellan-model-engine.coverage.json",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "prettier --ignore-path=../../.prettierignore  \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint -c ../../.eslintrc.js --ignore-path=../../.eslintignore --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/node": "^8.0.4",
    "chai": "^4.0.2",
    "chokidar-cli": "^1.2.0"
  },
  "dependencies": {
    "@types/chance": "^1.0.0",
    "@types/clone": "^0.1.30",
    "@types/lodash": "^4.14.106",
    "@types/shuffle-array": "0.0.28",
    "@types/type-detect": "^4.0.1",
    "@types/uuid": "^3.4.3",
    "async-file": "^2.0.2",
    "chance": "^1.0.10",
    "clone": "^1.0.4",
    "fs.realpath": "^1.0.0",
    "inflight": "^1.0.6",
    "lodash": "^4.17.4",
    "shuffle-array": "^1.0.1",
    "source-map-support": "^0.4.15",
    "type-detect": "^4.0.8",
    "uuid": "^3.2.1",
    "yn": "^2.0.0"
  }
}
