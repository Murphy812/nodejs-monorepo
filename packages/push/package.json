{
  "name": "push",
  "version": "1.0.0",
  "description": "push",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "tsc -b",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist && rm -f *.tsbuildinfo",
    "lint": "yarn run prettier:check && yarn run tslint",
    "lint:fix": "yarn run tslint:fix && yarn run prettier:fix",
    "prettier:cli": "prettier --ignore-path=../../.prettierignore  \"**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "tslint": "lb-tslint -c ../../tslint.json",
    "tslint:fix": "yarn run tslint --fix",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docker:build": "docker build -t sr2020/push -f Dockerfile ../..",
    "docker:push": "docker push sr2020/push",
    "migrate": "node -r tsconfig-paths/register ./src/migrate",
    "start": "node -r tsconfig-paths/register ./src"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {}
}
