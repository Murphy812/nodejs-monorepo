    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@loopback/build": "^6.2.2",
    "@loopback/eslint-config": "^6.0.1",
    "@loopback/testlab": "3.2.4",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "10.4.4",
    "@nrwl/cypress": "10.4.4",
    "@nrwl/eslint-plugin-nx": "10.4.4",
    "@nrwl/jest": "^10.4.4",
    "@nrwl/nest": "^10.4.4",
