{
  "name": "sr2020-nodejs",
  "version": "1.0.2",
  "description": "Monorepo for SR2020 parts written in NodeJS",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "clean": "find . -type f ( -name *.js -or -name *.map -or -name *.d.ts -or -name *.tsbuildinfo ) -not -path '*node_modules*' -print -delete",
    "build:watch": "tsc -b --watch",
    "coverage:coveralls": "nyc report --reporter=text-lcov | npx coveralls",
    "coverage:codecov": "npx codecov .nyc_output/",
    "presubmit": "lerna run lint && yarn build && lerna run test"
  },
  "repository": {
    "type": "git"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@loopback/context": "^3.1.0",
    "@loopback/core": "^2.1.0",
    "@loopback/openapi-v3": "^3.1.0",
    "@loopback/repository": "^2.0.1",
    "@loopback/rest": "^6.2.0",
    "@loopback/rest-explorer": "^2.0.1",
    "@loopback/service-proxy": "^2.0.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.7.5",
    "@types/dotenv": "^8.2.0",
    "@types/winston": "^2.4.4",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "document-register-element": "1.13.1",
    "dotenv": "^8.0.0",
    "loopback-connector-postgresql": "^5.1.0",
    "loopback-connector-rest": "^3.6.0",
    "moment": "^2.24.0",
    "react": "17.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5",
    "sql.js": "^1.4.0",
    "swagger-ui-express": "^4.1.5",
    "tsconfig-paths": "^3.8.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.18",
    "winston": "^3.2.1"
