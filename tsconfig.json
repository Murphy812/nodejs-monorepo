{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "@loopback/build/config/tsconfig.common.json",
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "noImplicitAny": false,
    "esModuleInterop": false,
    "baseUrl": "./packages",
    "paths": {
      "@sr2020/interface/*": [
        "interface/*"
      ],
      "@sr2020/alice-model-engine/*": [
        "alice-model-engine/*"
      ],
      "@sr2020/alice-models-manager/*": [
        "alice-models-manager/*"
      ],
      "@sr2020/sr2020-models-manager/*": [
        "sr2020-models-manager/*"
      ],
      "@sr2020/sr2020-model-engine/*": [
        "sr2020-model-engine/*"
      ],
      "@sr2020/sr2020-common/*": [
        "sr2020-common/*"
      ]
    }
  },
  "references": [
    {
      "path": "packages/utility-scripts"
    },
    {
      "path": "packages/alice-model-engine"
    },
    {
      "path": "packages/alice-qr-lib"
    },
    {
      "path": "packages/deus-model-engine"
    },
    {
      "path": "packages/interface"
    },
    {
      "path": "packages/magellan-model-engine"
    },
    {
      "path": "packages/alice-models-manager"
    },
    {
      "path": "packages/push"
    },
    {
      "path": "packages/sr2020-common"
    },
    {
      "path": "packages/sr2020-model-engine"
    },
    {
      "path": "packages/sr2020-models-manager"
    },
    {
      "path": "packages/sr2020-testing"
    },
    {
      "path": "packages/sheets-export"
    }
  ]
}
